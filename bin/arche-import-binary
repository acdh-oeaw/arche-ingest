#!/bin/bash
BDIR=`dirname $0`
BDIR=`realpath "$BDIR"`

if [ -d "$BDIR/../acdh-oeaw/arche-ingest" ] ; then
    COMPOSER_DIR="$BDIR/../.."
    BDIR="$BDIR/../acdh-oeaw/arche-ingest"
else
    COMPOSER_DIR="$BDIR/.."
    BDIR="$BDIR/.."
fi
export COMPOSER_DIR

if [ "$#" == "0" ] ; then
    php -f "$BDIR/import_binary_sample.php" -- -h
else
    php -f "$BDIR/import_binary_sample.php" -- $@
fi
